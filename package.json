{
  "name": "beautlog",
  "version": "0.3.1",
  "description": "Show log messages on your terminal with style :-) . Use debugging levels to enable or disable log messages.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/beautlog.git"
  },
  "keywords": [
    "log",
    "console.log",
    "terminal",
    "colors",
    "color",
    "logs",
    "logger",
    "logging",
    "debug",
    "cmd",
    "powershell",
    "command-line",
    "messages"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/beautlog/issues"
  },
  "homepage": "https://github.com/hebertcisco/beautlog#readme",
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "26.1.1",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "chalk": "^5.0.0",
    "color-regex": "^1.0.2",
    "date-handle": "^0.0.2",
    "log-beautify": "^1.2.0"
  }
}
