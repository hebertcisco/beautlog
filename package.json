{
  "name": "beautlog",
  "version": "0.5.7",
  "description": "Show log messages on your terminal with style :-) . Use debugging levels to enable or disable log messages.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "npx prettier --write \"src/**/*.(js|ts)\"",
    "lint": "npx eslint src --ext .js,.ts",
    "lint:fix": "npx eslint src --fix --ext .js,.ts",
    "test": "npx jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "prepare": "npx husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/beautlog.git"
  },
  "engines": {
    "node": ">=14.20.1",
    "npm": ">=6.14.17"
  },
  "keywords": [
    "log",
    "console.log",
    "terminal",
    "colors",
    "color",
    "logs",
    "logger",
    "logging",
    "debug",
    "cmd",
    "powershell",
    "command-line",
    "messages"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/beautlog/issues"
  },
  "homepage": "https://github.com/hebertcisco/beautlog#readme",
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.20.0",
    "eslint": "8.57.0",
    "eslint-plugin-jest": "28.6.0",
    "husky": "^9.0.2",
    "jest": "^29.5.0",
    "prettier": "3.3.0",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "colorette": "^2.0.20"
  }
}
